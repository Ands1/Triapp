<p id="notice"><%= notice %></p>

<h1>User Topic Answers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_topic_answers.each do |user_topic_answer| %>
      <tr>
        <td><%= link_to 'Show', user_topic_answer %></td>
        <td><%= link_to 'Edit', edit_user_topic_answer_path(user_topic_answer) %></td>
        <td><%= link_to 'Destroy', user_topic_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Topic Answer', new_user_topic_answer_path %>
